				"source": [
					"%%pyspark\r\n",
					"\r\n",
					"from pyspark.sql.functions import col, unix_timestamp\r\n",
					"\r\n",
					"df = spark.read.load('abfss://hevy@hevy.dfs.core.windows.net/Workout/workout_data.csv', format='csv'\r\n",
					"## If header exists uncomment line below\r\n",
					", header=True\r\n",
					")\r\n",
					"\r\n",
					"df = df.withColumn('start_time_unix', unix_timestamp(col('start_time'), 'd MMM yyyy, HH:mm').cast('timestamp'))\r\n",
					"df = df.withColumn('end_time_unix', unix_timestamp(col('end_time'), 'd MMM yyyy, HH:mm').cast('timestamp'))\r\n",
					"\r\n",
					"display(df.limit(10))\r\n",
					""
				],
				"execution_count": 32
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"from pyspark.sql.functions import split, col\n",
					"\n",
					"\n",
					"df = df.withColumn('time_difference_seconds', (unix_timestamp(col('end_time_unix')) - unix_timestamp(col('start_time_unix'))))\n",
					"\n",
					"\n",
					"display(df.limit(10))\n",
					"\n",
					""
				],
				"execution_count": 33
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"from pyspark.sql.functions import avg\n",
					"\n",
					"byDate = df.drop_duplicates([\"start_time\"])\n",
					"\n",
					"\n",
					"time = byDate.agg(avg(\"time_difference_seconds\").alias(\"time_difference_seconds\"))\n",
					"\n",
					"display(time)\n",
					"\n",
					""
				],
